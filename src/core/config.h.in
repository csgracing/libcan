#ifndef CONFIGURE_CONFIG_H_
#define CONFIGURE_CONFIG_H_

#define LIBCAN_PRODUCT_NAME "${CMAKE_PROJECT_NAME}"
#define LIBCAN_PRODUCT_VERSION "${CMAKE_PROJECT_VERSION}"

#cmakedefine01 LIBCAN_DEBUG

#define LIBCAN_LOG_LEVEL_NONE 0
#define LIBCAN_LOG_LEVEL_FATAL 1
#define LIBCAN_LOG_LEVEL_ERROR 2
#define LIBCAN_LOG_LEVEL_WARN 3
#define LIBCAN_LOG_LEVEL_INFO 4
#define LIBCAN_LOG_LEVEL_DEBUG 5
#define LIBCAN_LOG_LEVEL_VERBOSE 6

#if LIBCAN_DEBUG == 1
#define LIBCAN_LOG_LEVEL LIBCAN_LOG_LEVEL_VERBOSE
#else
#define LIBCAN_LOG_LEVEL LIBCAN_LOG_LEVEL_WARN
#endif

#endif /* CONFIGURE_CONFIG_H_ */